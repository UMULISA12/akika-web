#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/admin.py:12
msgid "Advanced options"
msgstr "Geavanceerde opties"

#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/apps.py:7
msgid "Flat Pages"
msgstr "Platte pagina's"

#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/forms.py:9
#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/models.py:12
msgid "URL"
msgstr "URL"

#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/forms.py:12
msgid ""
"Example: '/about/contact/'. Make sure to have leading and trailing slashes."
msgstr "Voorbeeld: '/about/contact/'. Zorg voor slashes aan het begin en eind."

#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/forms.py:15
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Deze waarde mag alleen letters, cijfers, punten, lage streepjes, streepjes, "
"schuine strepen of tildes bevatten."

#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/forms.py:29
msgid "URL is missing a leading slash."
msgstr "In de URL ontbreekt een begin slash."

#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/forms.py:37
msgid "URL is missing a trailing slash."
msgstr "In de URL ontbreekt een eind slash."

#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/forms.py:54
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "Platte pagina met url %(url)s bestaat al voor site %(site)s"

#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/models.py:13
msgid "title"
msgstr "titel"

#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/models.py:14
msgid "content"
msgstr "inhoud"

#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/models.py:15
msgid "enable comments"
msgstr "opmerkingen toestaan"

#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/models.py:17
msgid "template name"
msgstr "sjabloonnaam"

#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/models.py:21
msgid ""
"Example: 'flatpages/contact_page.html'. If this isn't provided, the system "
"will use 'flatpages/default.html'."
msgstr ""
"Voorbeeld: 'flatpages/contact_page.html'. Als deze niet is opgegeven, dan "
"wordt 'flatpages/default.html' gebruikt."

#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/models.py:26
msgid "registration required"
msgstr "registratie verplicht"

#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/models.py:27
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr ""
"Indien dit is aangevinkt kunnen alleen ingelogde gebruikers deze pagina "
"bekijken."

#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/models.py:30
msgid "sites"
msgstr "sites"

#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/models.py:34
msgid "flat page"
msgstr "platte pagina"

#: virtual/lib/python3.6/site-packages/django/contrib/flatpages/models.py:35
msgid "flat pages"
msgstr "platte pagina's"
